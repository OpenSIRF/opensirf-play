# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               				@controllers.Assets.versioned(path="/public", file: Asset)

POST	/addPreservationObjectInformation			@controllers.Application.addPreservationObjectInformation()

GET		/createPOI									@controllers.Application.createPreservationObjectInformation()

GET		/catalog									@controllers.Application.catalog()

POST	/saveCatalog								@controllers.Application.saveCatalog()

GET		/dynamicFormElements						@controllers.Application.dynamicFormElements(category: String, index: Int, subIndex: Int)

GET		/setup										@controllers.Application.setup()

GET		/											@controllers.Application.setup()

GET		/documentation								@controllers.Application.documentation()

GET		/about										@controllers.Application.about()

GET 	/saveSetup									@controllers.Application.saveSetup(endpoint: String)

GET		/editPreservationObject						@controllers.Application.editPreservationObject(uuid: String)

GET		/downloadPreservationObject					@controllers.Application.downloadPreservationObject(uuid: String)

POST	/savePreservationObjectInformation			@controllers.Application.savePreservationObjectInformation()