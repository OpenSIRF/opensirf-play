# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           @controllers.Application.index()

POST    /addExtension	            @controllers.Application.addExtension()

POST    /addFixityInformation       @controllers.Application.addFixityInformation()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               @controllers.Assets.versioned(path="/public", file: Asset)

GET		/extensionForm				@controllers.Application.extensionForm()

GET		/fixityInformationForm		@controllers.Application.fixityInformationForm()

GET		/extensionUnitTest			@controllers.Application.extensionUnitTest()

GET		/fixityInformationUnitTest			@controllers.Application.fixityInformationUnitTest()

GET		/preservationObjectAuditLogForm		@controllers.Application.preservationObjectAuditLogForm()

GET		/preservationObjectAuditLogUnitTest			@controllers.Application.preservationObjectAuditLogUnitTest()

POST    /addPreservationObjectAuditLog       @controllers.Application.addPreservationObjectAuditLog()

GET		/addPreservationObjectInformation	@controllers.Application.addPreservationObjectInformation()

GET		/poiUnitTest				@controllers.Application.poiUnitTest()