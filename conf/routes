# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           @controllers.Application.index()

POST    /addExtensionPair           @controllers.Application.addExtensionPair()

POST    /addExtension	            @controllers.Application.addExtension()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               @controllers.Assets.versioned(path="/public", file: Asset)

GET		/loadExtensionPairForm		@controllers.Application.loadExtensionPairForm()

GET		/loadExtensionForm			@controllers.Application.loadExtensionForm()